{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "0.1.0",
  "author": "{{ author }}",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./src",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --progress --config webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.config.js",
    "register": "node scripts/register",
    "update": "node scripts/update",
    "delete": "node scripts/delete",
    "publish": "node scripts/publish",
    "copyBinaryToTesting": "node scripts/copyBinaryToTesting",
    "test": "gherkin-testcafe chrome test/e2e/**.js test/e2e/**.feature --param-type-registry-file ./test/e2e/utils/custom-param-type-registry.js --skip-js-errors ",
    "report": "node ./script/report-generator.js",
    "preversion": "yarn --cwd \"scripts\" && yarn --cwd \"scripts\" --check-files && yarn install",
    "cucumber": "npm run build; npm run copyBinaryToTesting; gulp cucumber",
    "cucumberWindows": "npm run build && npm run copyBinaryToTesting && node updateChromeDriver && gulp cucumberWindows",
    "version": "yarn run build && git add .",
    "postversion": "git checkout master && git pull && git merge develop && git push origin master && git checkout develop && git push origin develop && git push --tags"
  },
  "dependencies": {
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
